<h1>Please enter your crews information!</h1>

<a href="/New_contacts">Click here to add your new crew member!</a>

@{
  if (Model == null )
  {
    <h3>Your feel is empty this is a sad day!</h3>
  }
  else
  {
    foreach (var Contact in Model)
    {
  <h3><a href='/index/@Contact.GetId()'>@Contact.GetContactName()</a></h3>
    }
  }
}
<p>
  <a href="/view_all">View all of your wonderful crew!</a>
</p>
