<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>
  <h1>Officers</h1>
  @if (@Model.Count == 0)
  {
    <h3>No Crew members have been added!</h3>
  }
  @foreach (var contact in Model)
  {
    <h3><a href='/officers/@contact.GetId()'>@contact.GetName()</a></h3>
  }
  <p>
    <a href="/clear_officers">Fire all Officers</a>
  </p>
  <p><a href='/'>Back home</a></p>
</body>
</html>
